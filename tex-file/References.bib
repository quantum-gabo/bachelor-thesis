  file = {/home/starlord/Zotero/storage/ZHC5WJNS/Pellenq et al. - 2009 - A realistic molecular model of cement hydrates.pdf}
}

@article{Perdew1981,
  title = {Self-Interaction Correction to Density-Functional Approximations for Many-Electron Systems},
  author = {Perdew, J. P. and Zunger, Alex},
  year = {1981},
