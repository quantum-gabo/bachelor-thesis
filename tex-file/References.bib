@article{Pellenq2009,
  title = {A Realistic Molecular Model of Cement Hydrates},
  author = {Pellenq, Roland J.-M. and Kushima, Akihiro and Shahsavari, Rouzbeh and Van Vliet, Krystyn J. and Buehler, Markus J. and Yip, Sidney and Ulm, Franz-Josef},
